<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="personalized.job.matcher.ReviewResultsUIController">
   <children>
      <TitledPane prefHeight="400.0" prefWidth="600.0" text="Personality Test Results">
         <content>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label fx:id="resultsLabel" alignment="CENTER" layoutX="39.0" layoutY="-3.0" prefHeight="32.0" prefWidth="521.0" text="Results" textFill="#1d28f2">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ListView fx:id="results" layoutX="13.0" layoutY="32.0" prefHeight="254.0" prefWidth="573.0" />
                  <Button fx:id="viewDetails" layoutX="194.0" layoutY="313.0" mnemonicParsing="false" text="View Details" />
                  <Button fx:id="exitButton" layoutX="353.0" layoutY="313.0" mnemonicParsing="false" onAction="#handleExitButtonAction" prefHeight="25.0" prefWidth="81.0" text="Exit" />
               </children>
            </AnchorPane>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
